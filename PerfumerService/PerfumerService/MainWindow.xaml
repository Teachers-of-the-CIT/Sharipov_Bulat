<Window x:Class="PerfumerService.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PerfumerService"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="productsButton" Click="productsButton_Click" Width="100" Height="100" Content="Просмотр товаров"/>
            <Button x:Name="createProductButton" Click="createProductButton_Click" Width="100" Height="100" Content="Добавить товар"/>
        </StackPanel> 
    </Grid>
</Window>
